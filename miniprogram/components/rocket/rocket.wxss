.toppiing {
  height: 70rpx;
  width: 70rpx;
  bottom: 150rpx;
  right: 30rpx;
  z-index: 12;
  position: fixed;
  border-radius: 18rpx;
  background-color: white;
  box-shadow: 0 1px 8px 0 rgba(0, 0, 0, 0.1);
  opacity: 0;
  transform: translateY(400px);
}

.toppiing_image {
  width: 100%;
  height: 100%;
}

.icon-rocket:before {
  content: '\e90c';
  font-family: 'icomoon' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

@import './rocket-wxa-auto-dark.wxss';
