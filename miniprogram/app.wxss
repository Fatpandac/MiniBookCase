@import './components/weui-wxss/dist/style/weui.wxss';
@import 'app-wxa-auto-dark.wxss';

page {
  font-family: Arial, Helvetica, sans-serif;
}

/* 隐藏滚动条 */
scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}

@font-face {
  font-family: 'icomoon';
  src: url('data:font/woff2;charset=utf-8;base64,')
      format('woff2'),
    url('data:font/woff;charset=utf-8;base64,')
      format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

[class^='icon-'],
[class*=' icon-'] {
  font-family: 'icomoon' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

